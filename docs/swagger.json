                "name",
                "nif",
                "password",
                "phone",
                "role",
                "status",
                "username"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "example": "joao@exemplo.com"
                },
                "name": {
                    "type": "string",
                    "minLength": 2,
                    "example": "João Silva"
                },
                "nif": {
                    "type": "string",
                    "example": "123456789"
                },
                "password": {
